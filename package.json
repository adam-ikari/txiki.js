{
  "name": "txiki.js",
  "version": "0.1.13",
  "description": "A small JavaScript runtime built on QuickJS",
  "private": false,
  "files": [
    "dist/linux-x64",
    "dist/linux-arm64",
    "dist/mac-x64",
    "dist/mac-m1",
    "dist/windows-x86",
    "dist/windows-x64",
    "dist/windows-arm32",
    "dist/windows-arm64"
  ],
  "scripts": {
    "build": "cmake -B build && cmake --build build -j$(nporc) && strip dist/linux-x64/tjs",
    "prepublishOnly": "yarn build"
  },
  "bin": {
    "tjs": "dist/linux-x64/tjs"
  },
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "abortcontroller-polyfill": "^1.7.5",
    "compression-streams-polyfill": "^0.1.7",
    "esbuild": "^0.21.5",
    "eslint": "^8.24.0",
    "eslint-plugin-import": "^2.26.0",
    "eventemitter3": "^5.0.1",
    "getopts": "^2.3.0",
    "ipaddr.js": "^2.2.0",
    "js-md5": "^0.8.3",
    "js-sha1": "^0.7.0",
    "js-sha256": "^0.9.0",
    "js-sha3": "^0.9.3",
    "js-sha512": "^0.9.0",
    "urlpattern-polyfill": "^10.0.0",
    "uuid": "10.0.0",
    "web-streams-polyfill": "^4.0.0",
    "gh-pages": "^5.0.0",
    "patch-package": "^6.4.7",
    "whatwg-url": "14.0.0",
    "typedoc": "0.26.3"
  }
}
